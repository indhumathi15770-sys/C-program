#include <stdio.h>
int main() {
    int ch;
    printf("1. Tea\n2. Coffee\n3. Milk\nEnter choice: ");
    scanf("%d", &ch);
    switch(ch) {
        case 1: printf("You selected Tea"); break;
        case 2: printf("You selected Coffee"); break;
        case 3: printf("You selected Milk"); break;
        default: printf("Invalid Choice");
    }
    return 0;
}
