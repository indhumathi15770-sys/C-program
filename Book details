#include <stdio.h>
struct book{
    int id;
    char title[20];
    float price;
};
int main(){
    struct book b[3];
    for(int i=0;i<3;i++)
        scanf("%d %s %f",&b[i].id,b[i].title,&b[i].price);
    for(int i=0;i<3;i++)
        printf("\n%d %s %.2f",b[i].id,b[i].title,b[i].price);
    return 0;
}
